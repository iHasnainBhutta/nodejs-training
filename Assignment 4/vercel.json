{
    "version": 2,
    "builds": [
      {
        "src": "app.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/products",
        "dest": "/api/products"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  