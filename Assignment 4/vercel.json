{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      { "src": "/api-docs", "dest": "/api-docs" },
      { "src": "/api/auth(.*)", "dest": "/routes/authRouter.js" },
      { "src": "/api/product(.*)", "dest": "/routes/productRouter.js" },
      { "src": "/(.*)", "dest": "/app.js" }
    ]
  }
  