{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/api-docs", "dest": "/api-docs" },
        { "src": "/api/auth(.*)", "dest": "/app.js" },
        { "src": "/api/products(.*)", "dest": "/app.js" },
        { "src": "/(.*)", "dest": "/app.js" }
    ]
  }
  